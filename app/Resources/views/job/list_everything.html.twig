{% extends 'base.html.twig' %}

{% block headerTitle %}Everything{% endblock %}

{% block breadcrumbs %}
    <li class="active"><span>Everything</span></li>
{% endblock %}

{% block afterBreadcrumbs %}
    <span style="float:right"><a href="{{ path('export_jobs', {'name': name, 'sales_order': sales_order, 'esd_date_from': esd_date_from, 'esd_date_to': esd_date_to, 'non_shipped': non_shipped, 'planner_esd_date_from': planner_esd_date_from, 'planner_esd_date_to': planner_esd_date_to, 'planner_esd_week_from': planner_esd_week_from, 'planner_esd_week_to': planner_esd_week_to}) }}" class="btn btn-sm btn-success">Export to CSV</a></span>
{% endblock %}

{% block body %}
    <div class="ibox float-e-margins">
        <div class="ibox-content">
            <form action="{{ path('everything') }}" method="GET">
                <div class="row">
                    <div class="col-sm-3">
                        <label class="form-label">Job Name</label>
                        <input type="text" name="name" value="{{ name }}" placeholder="Type in job name" class="form-control">
                    </div>
                    <div class="col-sm-3">
                        <label class="form-label">Sales Order #</label>
                        <input type="text" name="sales_order" value="{{ sales_order }}" placeholder="Type in SO #" class="form-control">
                    </div>                    
                    <div class="col-sm-4">
                        <label class="form-label">Estimated Ship Date Range</label>
                        <div class="input-daterange input-group">
                            <input type="text" id="esd_date_from" name="esd_date_from" value="{{ esd_date_from }}" class="form-control">
                            <span class="input-group-addon">to</span>
                            <input type="text" id="esd_date_to" name="esd_date_to" value="{{ esd_date_to }}" class="form-control">
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-4">
                        <label class="form-label">Planner ESD Date Range</label>
                        <div class="input-daterange input-group">
                            <input type="text" id="planner_esd_date_from" name="planner_esd_date_from" value="{{ planner_esd_date_from }}" class="form-control">
                            <span class="input-group-addon">to</span>
                            <input type="text" id="planner_esd_date_to" name="planner_esd_date_to" value="{{ planner_esd_date_to }}" class="form-control">
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <label class="form-label">Planner ESD Week Range</label>
                        <div class="input-daterange input-group">
                            <input type="text" name="planner_esd_week_from" value="{{ planner_esd_week_from }}" class="form-control">
                            <span class="input-group-addon">to</span>
                            <input type="text" name="planner_esd_week_to" value="{{ planner_esd_week_to }}" class="form-control">
                        </div>
                    </div>
                    <div class="col-sm-3" style="margin-top:20px">
                        <label class="form-check-label">
                            <input type="hidden" name="non_shipped" value="0" />
                            <input type="checkbox" name="non_shipped" value="1" class="form-check-input" {% if non_shipped %}checked{% endif %}>
                            See only non shipped jobs
                        </label>
                    </div>
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="pull-right">
                                <input type="submit" class="btn btn-info" style="margin-bottom:5px" value="Search">
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>

    <span style="font-size:70%"> (Total jobs: {{ jobs|length }})</span>
    <table class="table table-bordered">
        <thead>
        <tr>
            <th style="border:solid;width:10ch" rowspan=2>Name</th>
            <th style="border:solid;width:6ch" rowspan=2>SO #</th>
            <th style="border:solid;text-align:center" colspan=8>Job</th>
            <th style="border:solid;text-align:center" colspan=3>BOM</th>
            <th style="border:solid;text-align:center" colspan=4>Kitting</th>
            <th style="border:solid;text-align:center" colspan=7>Short #1</th>
            <th style="border:solid;text-align:center" colspan=7>Short #2</th>
            <th style="border:solid;text-align:center" colspan=7>Short #3</th>
            <th style="border:solid;text-align:center" colspan=7>Short #4</th>
            <th style="border:solid;text-align:center" colspan=5>Paint</th>
            <th style="border:solid;text-align:center" colspan=3>Scheduling</th>
            <th style="border:solid;text-align:center" colspan=4>Shipped</th>
            <th style="border:solid;" rowspan=2>&nbsp;</th>
            <th style="border:solid;" rowspan=2>Last Updated by</th>
        </tr>
        <tr>
            {# Job #}
            <th style="border:solid;min-width:7ch">ESD</th>
            <th style="border:solid;width:7ch;font-size:60%">Planner ESD (Week#)</th>
            <th style="border:solid;width:10ch">Type</th>
            <th style="border:solid;width:6ch">Model</th>
            <th style="border:solid;width:4ch">Qty</th>
            <th style="border:solid;width:6ch">MO#</th>
            <th style="border:solid;width:6ch">MAC PO#</th>
            <th style="border:solid;width:3ch">LOC</th>
            {# BOM #}
            <th style="border:solid;width:7ch;font-size:60%">Issued Date</th>
            <th style="border:solid;width:3ch;font-size:60%">Issued By</th>
            <th style="border:solid;width:7ch;font-size:80%">Serials Gen Date</th>
            {# Kitting #}
            <th style="border:solid;width:3ch">LOC</th>
            <th style="border:solid;width:7ch;font-size:80%">Date Kitted</th>
            <th style="border:solid;width:7ch;font-size:60%">Completion Date</th>
            <th style="border:solid;width:3ch;font-size:60%">Completed By</th>
            {# Short #1 #}
            <th style="border:solid;min-width:11ch">Part Number</th>
            <th style="border:solid;width:4ch;font-size:60%">Short Class</th>
            <th style="border:solid;min-width:7ch;font-size:60%">Date Needed</th>
            <th style="border:solid;min-width:7ch">EDD</th>
            <th style="border:solid;min-width:7ch">Mod Done Date</th>
            <th style="border:solid;min-width:7ch">RCVD Date</th>
            <th style="border:solid;width:20ch">Notes</th>
            {# Short #2 #}
            <th style="border:solid;min-width:11ch">Part Number</th>
            <th style="border:solid;width:4ch;font-size:60%">Short Class</th>
            <th style="border:solid;min-width:7ch;font-size:60%">Date Needed</th>
            <th style="border:solid;min-width:7ch">EDD</th>
            <th style="border:solid;min-width:7ch">Mod Done Date</th>
            <th style="border:solid;min-width:7ch">RCVD Date</th>
            <th style="border:solid;width:20ch">Notes</th>
            {# Short #3 #}
            <th style="border:solid;min-width:11ch">Part Number</th>
            <th style="border:solid;width:4ch;font-size:60%">Short Class</th>
            <th style="border:solid;min-width:7ch;font-size:60%">Date Needed</th>
            <th style="border:solid;min-width:7ch">EDD</th>
            <th style="border:solid;min-width:7ch">Mod Done Date</th>
            <th style="border:solid;min-width:7ch">RCVD Date</th>
            <th style="border:solid;width:20ch">Notes</th>
            {# Short #4 #}
            <th style="border:solid;min-width:11ch">Part Number</th>
            <th style="border:solid;width:4ch;font-size:60%">Short Class</th>
            <th style="border:solid;min-width:7ch;font-size:60%">Date Needed</th>
            <th style="border:solid;min-width:7ch">EDD</th>
            <th style="border:solid;min-width:7ch">Mod Done Date</th>
            <th style="border:solid;min-width:7ch">RCVD Date</th>
            <th style="border:solid;width:20ch">Notes</th>
            {# Paint #}
            <th style="border:solid;width:4ch">LOC</th>
            <th style="border:solid;width:4ch">Color 1</th>
            <th style="border:solid;width:4ch">Batch 1</th>
            <th style="border:solid;width:4ch">Color 2</th>
            <th style="border:solid;width:4ch">Batch 2</th>
            {# Scheduling #}
            <th style="border:solid;width:4ch">Sub Ready</th>
            <th style="border:solid;min-width:7ch;font-size:60%">Build Completion Date</th>
            <th style="border:solid;width:4ch">Priority</th>
            {# Shipped #}
            <th style="border:solid;width:7ch">1st Ship Date </th>
            <th style="border:solid;width:4ch">Complete or Partial</th>
            <th style="border:solid;width:7ch">2nd Ship Date</th>
            <th style="border:solid;width:7ch">Notes</th>
        </tr>            
        </thead>
        <tbody>
            {% for job in jobs %}
            <tr {% if job.formatting == 1 %}bgcolor="lightgreen"{% elseif job.formatting == 2 %}bgcolor="lightcoral"{% elseif job.formatting == 3 %}bgcolor="lightyellow"{% endif %}>
                {# Job #}
                <td style="font-size:small; {% if job.formatting > 0 %}color:white{% endif %}"><a href="#" class="name" data-name="name" data-pk="{{job.id}}" data-url="{{ path('edit_job_name', {'jobId': job.id}) }}">{{ job.name }}</a></td>
                <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="salesOrder" data-name="salesOrder" data-pk="{{job.id}}" data-url="{{ path('edit_job_sales_order', {'jobId': job.id}) }}">{{ job.salesOrder }}</a></td>   
                <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="estimatedShipDate" data-name="estimatedShipDate" data-pk="{{job.id}}" data-url="{{ path('edit_job_estimated_ship_date', {'jobId': job.id}) }}">{{ job.estimatedShipDate ? job.estimatedShipDate | date('Y-m-d') : "" }}</a></td>
                <td {% if job.formatting > 0 %}style="color:white"{% endif %}>
                    <a href="#" class="plannerEstimatedShipDate" data-name="plannerEstimatedShipDate" data-pk="{{job.id}}" data-url="{{ path('edit_job_planner_estimated_ship_date', {'jobId': job.id}) }}">
                        {% if job.plannerEstimatedShipDate %}
                            {{ job.plannerEstimatedShipDate|date('Y-m-d') }} ({{ job.plannerEstimatedShipDate|date('W') }})
                        {% else %}
                            TBD
                        {% endif %}
                    </a>
                </td>                
                <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="type" data-name="type" data-pk="{{job.id}}" data-url="{{ path('edit_job_type', {'jobId': job.id}) }}">{{ job.type }}</a></td>
                <td style="word-break:break-all"><a href="#" class="model" data-name="model" data-pk="{{job.id}}" data-url="{{ path('edit_job_model', {'jobId': job.id}) }}">{{ job.model }}</a></td>
                <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="quantity" data-name="quantity" data-pk="{{job.id}}" data-url="{{ path('edit_job_quantity', {'jobId': job.id}) }}">{{ job.quantity }}</a></td>
                <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="manufacturingOrder" data-name="manufacturingOrder" data-pk="{{job.id}}" data-url="{{ path('edit_job_manufacturing_order', {'jobId': job.id}) }}">{{ job.manufacturingOrder }}</a></td>                    
                <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="macPurchaseOrder" data-name="macPurchaseOrder" data-pk="{{job.id}}" data-url="{{ path('edit_job_mac_purchase_order', {'jobId': job.id}) }}">{{ job.macPurchaseOrder }}</a></td>
                <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="buildLocation" data-name="buildLocation" data-pk="{{job.id}}" data-value="{{job.buildLocation.id}}" data-url="{{ path('edit_job_build_location', {'jobId': job.id}) }}">{{ job.buildLocation.name }}</a></td>
                {# BOM #}
                <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="issuedDate" data-name="issuedDate" data-pk="{{job.id}}" data-url="{{ path('edit_bom_issued_date', {'jobId': job.id}) }}">{{ job.bom and job.bom.issuedDate ? job.bom.issuedDate|date('Y-m-d') : "" }}</a></td>
                <td {% if job.formatting > 0 %}style="color:white"{% endif %}>{{ job.bom and job.bom.issuedBy ? job.bom.issuedBy.initials : ""}}</td>
                <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="serialsGeneratedDate" data-name="serialsGeneratedDate" data-pk="{{job.id}}" data-url="{{ path('edit_bom_serials_generated_date', {'jobId': job.id}) }}">{{ job.bom and job.bom.serialsGeneratedDate ? job.bom.serialsGeneratedDate|date('Y-m-d') : "" }}</a></td>
                {# Kitting #}
                <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="location" data-name="location" data-pk="{{job.kitting ? job.kitting.id : 0}}" data-url="{{ path('edit_kitting_location', {'jobId': job.id}) }}">{{ job.kitting ? job.kitting.location : '' }}</a></td>              
                <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" id="kitDate_{{job.kitting ? job.kitting.id : 0}}" class="kitDate" data-name="kitDate" data-pk="{{job.kitting ? job.kitting.id : 0}}" data-url="{{ path('edit_kitting_kit_date', {'jobId': job.id}) }}">{{job.kitting and job.kitting.kitDate ? job.kitting.kitDate | date('Y-m-d') : ""}}</a></td>
                <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="kittingCompletionDate" data-name="kittingCompletionDate" data-pk="{{job.id}}" data-url="{{ path('edit_kitting_completion_date', {'jobId': job.id}) }}">{{ job.kitting and job.kitting.completionDate ? job.kitting.completionDate | date('Y-m-d') : ""}}</a></td>
                <td {% if job.formatting > 0 %}style="color:white"{% endif %}>{{ job.kitting ? job.kitting.updatedBy.initials : "" }}</td>
                {# Short #1 #}
                {% if job.kitting and job.kitting.kittingShort1 is null %}
                    <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="shortPartNumber" data-name="shortPartNumber" data-pk="{{job.kitting ? job.kitting.id}}" data-value="" data-url="{{ path('edit_kitting_short1', {'jobId': job.id, 'shortId': 0}) }}"></a></td>
                    <td></td><td></td><td></td><td></td><td></td><td></td>
                {% else %}
                    <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="shortPartNumber" data-name="shortPartNumber" data-pk="{{job.kitting ? job.kitting.id}}" data-value="{{job.kitting and job.kitting.kittingShort1 ? job.kitting.kittingShort1.partNumber}}" data-url="{{ path('edit_kitting_short1', {'jobId': job.id, 'shortId': (job.kitting and job.kitting.kittingShort1) ? job.kitting.kittingShort1.id : 0}) }}">{{ job.kitting and job.kitting.kittingShort1 ? job.kitting.kittingShort1.partNumber : "" }}</a></td>
                    <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="shortClass" data-name="shortClass" data-pk="{{job.kitting ? job.kitting.id : 0}}" data-value="{{job.kitting and job.kitting.kittingShort1 ? job.kitting.kittingShort1.shortClass : 0}}" data-url="{{ path('edit_kitting_short_short_class', {'shortId': (job.kitting and job.kitting.kittingShort1) ? job.kitting.kittingShort1.id : 0}) }}">
                        {% if job.kitting.kittingShort1.shortClass == 0 %}
                            Empty
                        {% elseif job.kitting.kittingShort1.shortClass == 1 %}
                            Painted
                        {% elseif job.kitting.kittingShort1.shortClass == 2 %}
                            Ignore
                        {% endif %}
                    </a></td>
                    <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="dateNeeded" data-name="dateNeeded" data-pk="{{job.kitting ? job.kitting.id : 0}}" data-url="{{ path('edit_kitting_short_date_needed', {'shortId': (job.kitting and job.kitting.kittingShort1) ? job.kitting.kittingShort1.id : 0}) }}">{{ job.kitting and job.kitting.kittingShort1 and job.kitting.kittingShort1.dateNeeded ? job.kitting.kittingShort1.dateNeeded | date('Y-m-d') : "" }}</a></td>
                    <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="estimatedDeliveryDate" data-name="estimatedDeliveryDate" data-pk="{{ job.kitting ? job.kitting.id }}" data-url="{{ path('edit_kitting_short_estimated_delivery_date', { 'shortId': (job.kitting and job.kitting.kittingShort1) ? job.kitting.kittingShort1.id : 0}) }}">{{ job.kitting and job.kitting.kittingShort1 and job.kitting.kittingShort1.estimatedDeliveryDate ? job.kitting.kittingShort1.estimatedDeliveryDate | date('Y-m-d') : "" }}</a></td>
                    <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="modDoneDate" data-name="modDoneDate" data-pk="{{ job.kitting ? job.kitting.id }}" data-url="{{ path('edit_kitting_short_mod_done_date', { 'shortId': (job.kitting and job.kitting.kittingShort1) ? job.kitting.kittingShort1.id : 0}) }}">{{ job.kitting and job.kitting.kittingShort1 and job.kitting.kittingShort1.modDoneDate ? job.kitting.kittingShort1.modDoneDate | date('Y-m-d') : "" }}</a></td>
                    <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="receivedDate" data-name="receivedDate" data-pk="{{ job.kitting ? job.kitting.id }}" data-url="{{ path('edit_kitting_short_received_date', { 'shortId': (job.kitting and job.kitting.kittingShort1) ? job.kitting.kittingShort1.id : 0}) }}">{{ job.kitting and job.kitting.kittingShort1 and job.kitting.kittingShort1.receivedDate ? job.kitting.kittingShort1.receivedDate | date('Y-m-d') : "" }}</a></td>
                    <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="notes" data-name="notes" data-pk="{{job.kitting ? job.kitting.id : 0}}" data-url="{{ path('edit_kitting_short_notes', {'jobId': job.id, 'shortId': (job.kitting and job.kitting.kittingShort1) ? job.kitting.kittingShort1.id : 0}) }}">{{ job.kitting and job.kitting.kittingShort1 ? job.kitting.kittingShort1.notes : null }}</a></td>
                {% endif %}
                {# Short #2 #}
                {% if job.kitting and job.kitting.kittingShort2 is null %}
                    <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="shortPartNumber" data-name="shortPartNumber" data-pk="{{job.kitting ? job.kitting.id : 0}}" data-value="{{job.kitting.kittingShort2 ? job.kitting.kittingShort2.partNumber}}" data-url="{{ path('edit_kitting_short2', {'jobId': job.id, 'shortId': (job.kitting and job.kitting.kittingShort2) ? job.kitting.kittingShort2.id : 0}) }}">{{ job.kitting.kittingShort2 ? job.kitting.kittingShort2.partNumber : "" }}</a></td>
                    <td></td><td></td><td></td><td></td><td></td><td></td>
                {% else %}
                    <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="shortPartNumber" data-name="shortPartNumber" data-pk="{{job.kitting ? job.kitting.id : 0}}" data-value="{{job.kitting and job.kitting.kittingShort2 ? job.kitting.kittingShort2.partNumber}}" data-url="{{ path('edit_kitting_short2', {'jobId': job.id, 'shortId': (job.kitting and job.kitting.kittingShort2) ? job.kitting.kittingShort2.id : 0}) }}">{{ job.kitting and job.kitting.kittingShort2 ? job.kitting.kittingShort2.partNumber : "" }}</a></td>
                    <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="shortClass" data-name="shortClass" data-pk="{{job.kitting ? job.kitting.id : 0}}" data-value="{{job.kitting and job.kitting.kittingShort2 ? job.kitting.kittingShort2.shortClass : 0}}" data-url="{{ path('edit_kitting_short_short_class', {'shortId': (job.kitting and job.kitting.kittingShort2) ? job.kitting.kittingShort2.id : 0}) }}">
                        {% if job.kitting.kittingShort2.shortClass == 0 %}
                            Empty
                        {% elseif job.kitting.kittingShort2.shortClass == 1 %}
                            Painted
                        {% elseif job.kitting.kittingShort2.shortClass == 2 %}
                            Ignore
                        {% endif %}
                    </a></td>
                    <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="dateNeeded" data-name="dateNeeded" data-pk="{{job.kitting ? job.kitting.id : 0}}" data-url="{{ path('edit_kitting_short_date_needed', {'shortId': (job.kitting and job.kitting.kittingShort2) ? job.kitting.kittingShort2.id : 0}) }}">{{ job.kitting and job.kitting.kittingShort2 and job.kitting.kittingShort2.dateNeeded ? job.kitting.kittingShort2.dateNeeded | date('Y-m-d') : "" }}</a></td>
                    <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="estimatedDeliveryDate" data-name="estimatedDeliveryDate" data-pk="{{ job.kitting ? job.kitting.id }}" data-url="{{ path('edit_kitting_short_estimated_delivery_date', { 'shortId': (job.kitting and job.kitting.kittingShort2) ? job.kitting.kittingShort2.id : 0}) }}">{{ job.kitting and job.kitting.kittingShort2 and job.kitting.kittingShort2.estimatedDeliveryDate ? job.kitting.kittingShort2.estimatedDeliveryDate | date('Y-m-d') : "" }}</a></td>
                    <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="modDoneDate" data-name="modDoneDate" data-pk="{{ job.kitting ? job.kitting.id }}" data-url="{{ path('edit_kitting_short_mod_done_date', { 'shortId': (job.kitting and job.kitting.kittingShort2) ? job.kitting.kittingShort2.id : 0}) }}">{{ job.kitting and job.kitting.kittingShort2 and job.kitting.kittingShort2.modDoneDate ? job.kitting.kittingShort2.modDoneDate | date('Y-m-d') : "" }}</a></td>
                    <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="receivedDate" data-name="receivedDate" data-pk="{{ job.kitting ? job.kitting.id }}" data-url="{{ path('edit_kitting_short_received_date', { 'shortId': (job.kitting and job.kitting.kittingShort1) ? job.kitting.kittingShort1.id : 0}) }}">{{ job.kitting and job.kitting.kittingShort2 and job.kitting.kittingShort2.receivedDate ? job.kitting.kittingShort2.receivedDate | date('Y-m-d') : "" }}</a></td>
                    <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="notes" data-name="notes" data-pk="{{job.kitting ? job.kitting.id : 0}}" data-url="{{ path('edit_kitting_short_notes', {'jobId': job.id, 'shortId': (job.kitting and job.kitting.kittingShort2) ? job.kitting.kittingShort2.id : 0}) }}">{{ job.kitting and job.kitting.kittingShort2 ? job.kitting.kittingShort2.notes : null }}</a></td>
                {% endif %}
                {# Short #3 #}
                {% if job.kitting and job.kitting.kittingShort3 is null %}
                    <td {% if job.formatting > 0 %}style="color:white"{% endif %}><a href="#" class="shortPartNumber" data-name="shortPartNumber" data-pk="{{job.kitting ? job.kitting.id : 0}}" data-value="{{job.kitting.kittingShort3 ? job.kitting.kittingShort3.partNumber}}" data-url="{{ path('edit_kitting_short3', {'jobId': job.id, 'shortId': (job.kitting and job.kitting.kittingShort3) ? job.kitting.kittingShort3.id : 0}) }}">{{ job.kitting and job.kitting.kittingShort3 ? job.kitting.kittingShort3.partNumber : "" }}</a></td>
                    <td></td><td></td><td></td><td></td><td></td><td></td>
                {% else %}
