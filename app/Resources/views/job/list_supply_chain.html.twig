            <th style="width:100px;border:solid">Date Needed</th>
            <th style="width:100px;border:solid">Vendor</th>
            <th style="width:100px;border:solid">Vendor PO#</th>
            <th style="width:100px;border:solid">Mod WO#</th>
            <th style="width:100px;border:solid">EDD</th>
